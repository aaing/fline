<div class="row form">
  <div class="col-lg-12">
    <h3>Member List</h3>
    <div class="list-group">
    <% @room.users.each do |user| %>
      <% if user != current_user %>
        <div class="list-group-item"><%= link_to user.name, user_path(user) %><hr></div>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
